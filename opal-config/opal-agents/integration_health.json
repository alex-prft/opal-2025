      "schema_compliance": "Output matches ResultsPageContent interface with system-specific metrics (overall health score, system availability, integration issues)",
      "data_flow": "System health intelligence flows through osa_store_workflow_data → osa_send_data_to_osa_webhook → Results page dxptools tier",
      "tier_classification": "dxptools tier for comprehensive system orchestration and integration health monitoring",
      "orchestration_metrics_structure": "Hero metrics focus on system health scores, availability percentages, and active issue counts based on actual system performance",
      "master_orchestrator_role": "Coordinates all agent outputs, manages Canvas creation across agents, and provides predictive intelligence for optimal workflow routing"
    },
    "agent_role_in_ecosystem": {
      "primary_function": "Master orchestrator managing dual-mode system intelligence, multi-agent coordination, and OSA brain learning from user interactions",
      "data_sources": "System performance metrics from all DXP platforms, agent success rates, user interaction patterns, Canvas usage analytics, CMP workflow status",
      "output_consumers": "OSA Results pages dxptools section, system health dashboards, Canvas orchestration intelligence, predictive agent routing system",
      "coordination_capabilities": "Orchestrates all other agents via osa_orchestrate_multi_agent_workflows, manages failover with osa_manage_failover_routing, provides predictive intelligence with osa_predict_agent_routing_needs",
      "advanced_intelligence": "Implements OSA brain learning with cross-session intelligence analysis, user feedback loop processing, and automated system optimization based on usage patterns"
    }
