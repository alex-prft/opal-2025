    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.80.0",
    "@types/pg": "^8.15.6",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.21",
    "avsc": "^5.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "googleapis": "^164.1.0",
    "ioredis": "^5.8.2",
    "jose": "^6.1.0",
    "js-yaml": "^4.1.1",
    "jsforce": "^3.10.8",
    "kafkajs": "^2.2.4",
    "lucide-react": "^0.552.0",
    "next": "16.0.1",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "prom-client": "^15.1.3",
    "protobufjs": "^7.5.4",
    "puppeteer": "^24.28.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "recharts": "^3.4.1",
    "styled-jsx": "^5.1.7",
    "swagger-ui-dist": "^5.30.2",
    "swagger-ui-react": "^5.30.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.18",
    "tailwindcss-animate": "^1.0.7",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/swagger-ui-react": "^5.18.0",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vitest": "^1.6.0"
  }
}
