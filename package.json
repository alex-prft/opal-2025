{
  "name": "osa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:integration": "jest tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "setup:env": "node scripts/setup-env.js",
    "setup:env:generate": "node scripts/setup-env.js --generate",
    "setup:env:validate": "node scripts/setup-env.js --validate",
    "validate:opal": "npx tsx scripts/validate-opal-discovery.ts",
    "validate:opal:prod": "npx tsx scripts/validate-opal-discovery.ts --base-url=https://opal-2025.vercel.app",
    "validate:opal:main": "npx tsx scripts/validate-opal-discovery.ts --url=https://opal-2025.vercel.app/api/opal/discovery",
    "validate:opal:tool": "npx tsx scripts/validate-opal-discovery.ts --tool=",
    "validate:opal:url": "npx tsx scripts/validate-opal-discovery.ts --url=",
    "test:opal:fix": "vitest run tests/unit/opal-discovery-fix.test.ts",
    "validate:security": "npx tsx scripts/validate-security.ts",
    "validate:security:ci": "npx tsx scripts/validate-security.ts --ci",
    "validate:deployment": "npx tsx scripts/validate-deployment.ts",
    "validate:jsx-parsing": "node scripts/jsx-parsing-validator.js",
    "validate:all": "npm run validate:runtime-errors && npm run validate:security && npm run validate:deployment && npm run validate:jsx-parsing && npm run validate:opal && npm run validate:sop-all",
    "error-check": "node scripts/error-prevention.js",
    "error-check:fix": "node scripts/error-prevention.js --auto-fix",
    "validate:runtime-errors": "node scripts/runtime-error-prevention.js",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:error-prevention": "npm run test:unit tests/unit/",
    "test:runtime-error-prevention": "node tests/unit/runtime-error-prevention.test.js",
    "test:jsx-regression": "node tests/unit/jsx-parsing-regression-test.js",
    "pre-commit": "npm run validate:runtime-errors && npm run error-check && npm run validate:jsx-parsing && npm run test:jsx-regression",
    "pre-deploy": "npm run validate:runtime-errors && npm run error-check && npm run validate:jsx-parsing && npm run sop:validate-production && npm run validate:all",
    "start:opal-tools": "npx ts-node --project tsconfig.sdk.json scripts/start-sdk-tools.ts",
    "start:production-tools": "FALLBACK_TO_BASIC=true npx ts-node scripts/start-production-sdk-tools.ts",
    "start:production-tools:full": "npx ts-node scripts/start-production-sdk-tools.ts",
    "deploy:prod": "./scripts/deploy-prod.sh",
    "kafka:init": "./scripts/kafka-init.sh",
    "kafka:test-producer": "npx tsx scripts/kafka-test-producer.ts",
    "kafka:start-diagnostics": "npx tsx scripts/start-diagnostics-consumer.ts",
    "kafka:start-recommendations": "npx tsx scripts/start-recommendations-consumer.ts",
    "test:database-integration": "npx tsx scripts/test-database-integration.ts",
    "test:sop-compliance": "jest __tests__/sop-compliance/",
    "test:no-mock-data": "jest __tests__/sop-compliance/no-mock-data.test.tsx",
    "test:pageId-validation": "jest __tests__/sop-compliance/pageId-validation.test.tsx",
    "test:agent-orchestration": "jest __tests__/sop-compliance/agent-orchestration.test.tsx",
    "validate:sop-all": "npm run test:sop-compliance && npm run validate:opal-mappings",
    "validate:opal-mappings": "node scripts/validate-opal-mappings.js",
    "sop:validate-production": "NODE_ENV=production npm run test:sop-compliance",
    "sop:check-compliance": "npm run test:sop-compliance --silent && echo '✅ SOP Compliance: PASS' || echo '❌ SOP Compliance: FAIL'",
    "governance:deploy": "npx tsx scripts/deploy-supabase-governance.ts",
    "governance:health": "curl -s http://localhost:3000/api/admin/governance?metric=health | jq",
    "governance:status": "curl -s http://localhost:3000/api/admin/governance | jq '.overall_status'",
    "governance:pii-scan": "curl -s http://localhost:3000/api/admin/pii-scan | jq",
    "governance:purge-check": "curl -s http://localhost:3000/api/admin/governance?metric=purge-status | jq '.purge_metrics.records_eligible_for_purge'",
    "governance:validate-all": "npm run governance:health && npm run governance:pii-scan && npm run governance:purge-check",
    "guardrails:init": "npx tsx scripts/test-guardrails-integration.ts",
    "guardrails:test": "npx tsx scripts/test-guardrails-integration.ts",
    "guardrails:health": "curl -s http://localhost:3000/api/admin/guardrails-health | jq",
    "guardrails:health-detailed": "curl -s http://localhost:3000/api/admin/guardrails-health?detailed=true | jq",
    "guardrails:test-api": "curl -X POST -H 'Content-Type: application/json' -d '{\"action\": \"test_all\"}' http://localhost:3000/api/admin/test-guardrails | jq",
    "guardrails:migrate": "npx tsx scripts/migrate-to-secure-supabase.ts",
    "supabase:setup": "node scripts/apply-supabase-migrations.js",
    "supabase:setup-and-test": "npm run supabase:setup && npm run guardrails:test",
    "guardrails:migrate-analyze": "npx tsx scripts/migrate-to-secure-supabase.ts analyze",
    "guardrails:migrate-execute": "npx tsx scripts/migrate-to-secure-supabase.ts migrate --confirm",
    "guardrails:retention-status": "curl -s http://localhost:3000/api/admin/guardrails-health?detailed=true | jq '.detailed.retention_status'",
    "guardrails:run-retention": "curl -X POST -H 'Content-Type: application/json' -d '{\"action\": \"retention\", \"dryRun\": true}' http://localhost:3000/api/admin/guardrails-health | jq",
    "guardrails:compliance-report": "curl -X POST -H 'Content-Type: application/json' -d '{\"action\": \"compliance_report\", \"startDate\": \"2024-01-01\", \"endDate\": \"2024-12-31\"}' http://localhost:3000/api/admin/guardrails-health | jq"
  },
  "dependencies": {
    "@google-analytics/data": "^5.2.1",
