    "@supabase/supabase-js": "^2.80.0",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/pg": "^8.15.6",
    "@types/ws": "^8.18.1",
    "autoprefixer": "^10.4.21",
    "avsc": "^5.7.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "date-fns": "^4.1.0",
    "googleapis": "^164.1.0",
    "ioredis": "^5.8.2",
    "jose": "^6.1.0",
    "js-yaml": "^4.1.1",
    "jsforce": "^3.10.8",
    "kafkajs": "^2.2.4",
    "lucide-react": "^0.552.0",
    "next": "16.0.1",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
